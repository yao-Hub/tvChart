<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <!-- 子进程强制沙盒 -->
  <key>com.apple.security.app-sandbox</key>
  <true/>
  
  <!-- 允许辅助进程通信 -->
  <key>com.apple.security.inherit</key>
  <true/>
</dict>
</plist>