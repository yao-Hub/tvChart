{
  "name": "ctotrader",
  "private": true,
  "version": "2.0.0",
  "main": "electron/main.js",
  "description": "CTOTrader",
  "author": "Furong Uptech Solution Co., Ltd",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --open",
    "staging": "vite --mode staging --open",
    "prod": "vite --mode production --open",
    "prepackage": "npx tsc scripts/update-version.ts && node scripts/update-version.js && git add package.json",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --open",
    "preview:dev": "vite preview --mode development --open",
    "lint": "eslint --ext .js,.vue,.ts src",
    "http-server": "cd dist && http-server -c-1 -p 5000 -P http://localhost:8080 -o",
    "ts:check": "vue-tsc",
    "prepare": "husky",
    "electron:dev": "cross-env IF_ELECTRON=true vite --mode development",
    "electron:staging": "cross-env IF_ELECTRON=true NODE_ENV=staging vite --mode staging",
    "electron:prod": "cross-env IF_ELECTRON=true NODE_ENV=production vite --mode production",
    "electron-build:dev": "npm run prepackage && cross-env NODE_ENV=development IF_ELECTRON=true node ./electron/scripts/build.js",
    "electron-build:staging": "npm run prepackage && cross-env NODE_ENV=staging IF_ELECTRON=true node ./electron/scripts/build.js",
    "electron-build:prod": "npm run prepackage && cross-env NODE_ENV=production IF_ELECTRON=true node ./electron/scripts/build.js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "7.0.1",
    "@element-plus/icons-vue": "2.3.1",
    "@types/node": "22.7.5",
    "@vueuse/core": "11.0.0",
    "axios": "1.7.9",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.11",
    "decimal.js": "10.4.3",
    "downloads-folder": "3.0.3",
    "electron-updater": "6.6.2",
    "element-plus": "2.8.4",
    "inquirer": "12.4.1",
    "pinia": "2.1.7",
    "simple-git": "3.27.0",
    "socket.io": "4.7.5",
    "socket.io-client": "4.7.5",
    "sortablejs": "1.15.2",
    "vue": "3.4.21",
    "vue-clipboard3": "2.0.0",
    "vue-draggable-plus": "0.5.1",
    "vue-i18n": "^10.0.5",
    "vue-router": "4.3.2"
  },
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "@rollup/plugin-strip": "3.0.4",
    "@types/crypto-js": "4.2.2",
    "@types/lodash": "4.17.1",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.8",
    "cross-env": "7.0.3",
    "electron": "28.0.0",
    "electron-builder": "25.1.8",
    "eslint": "9.2.0",
    "eslint-plugin-vue": "9.25.0",
    "globals": "15.1.0",
    "husky": "9.1.7",
    "less": "4.2.0",
    "less-loader": "12.2.0",
    "lodash": "4.17.21",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.76.0",
    "terser": "5.31.0",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.28.0",
    "unplugin-vue-components": "0.27.0",
    "vite": "5.2.0",
    "vite-plugin-electron": "0.29.0",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-node-polyfills": "0.22.0",
    "vue-tsc": "2.0.6"
  },
  "keywords": [
    "CTOTrader",
    "utrader"
  ],
  "engines": {
    "node": "20.15.0"
  }
}